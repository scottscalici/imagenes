  <p class="text-xs text-gray-500 mt-2">Enter = revisar campo actual · Tab = siguiente campo</p>
</section>

<script>
(function(){
  const sect = document.currentScript.previousElementSibling; // section
  const inputs = sect.querySelectorAll('.cloze-inp');
  const checkBtn = sect.querySelector('.cloze-check');
  const resetBtn = sect.querySelector('.cloze-reset');
  function norm(s){return (s||'').trim();}
  function checkOne(el){
    const ok = norm(el.value) === norm(el.dataset.answer);
    el.classList.remove('ring-2','ring-red-400','ring-emerald-400');
    el.classList.add('ring-2', ok ? 'ring-emerald-400' : 'ring-red-400');
    return ok;
  }
  inputs.forEach((inp)=>{
    inp.addEventListener('keydown',(e)=>{
      if(e.key==='Enter'){ e.preventDefault(); checkOne(inp); }
    });
  });
  checkBtn.addEventListener('click',()=> inputs.forEach(checkOne));
  resetBtn.addEventListener('click',()=>{
    inputs.forEach(i=>{ i.value=''; i.classList.remove('ring-2','ring-red-400','ring-emerald-400');});
    inputs[0]?.focus();
  });
})();
</script><footer class='text-center text-xs text-gray-500 pt-6'>Creado para clase · Cuarteto de nos – “Ya no sé que hacer conmigo”</footer></div></body></html>
